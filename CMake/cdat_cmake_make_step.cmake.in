
if(NOT APPLE)
  include(@cdat_CMAKE_BINARY_DIR@/cdat_common_environment.cmake)
endif()

execute_process(
  COMMAND ${make}
  WORKING_DIRECTORY "${WORKING_DIR}"
  RESULT_VARIABLE res)

if(NOT ${res} EQUAL 0)
  message("Make Errors detected: \n${CDAT_OUT}\n${CDAT_ERR}")
  message(FATAL_ERROR "Error in Make")
endif()
message("Make succeeded.")

